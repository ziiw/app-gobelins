#menu
	background-color blue
	width 100%
	height 100%
	z-index 1
	position fixed
	top 0px
	left 0px
	
	ul
		width 200px
		position absolute
		left 15%
		top 55%
		transform translate(-15%, -50%)
		
		li
			font-size 16px
			color white
			text-align center
			text-transform uppercase
			margin-bottom 40px
			
			a
				color white
				text-decoration none
	
	img
		width 100px